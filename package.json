{
  "name": "vs-ui",
  "version": "0.0.20",
  "description": "An Vue UI Toolkit For Mobile",
  "scripts": {
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit --watch",
    "lint": "vue-cli-service lint",
    "deploy": "sh build/deploy.sh",
    "bootstrap": "npm install && cd ./packages/theme-chalk && npm install",
    "build:bundle": "webpack --config ./build/webpack.component.js",
    "build:docs": "vue-cli-service build",
    "build:entry": "node build/build-entry.js ",
    "build:lib": "node build/build-lib.js",
    "build:style": "gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk && cp-cli packages/theme-chalk/src/imgs lib/theme-chalk/imgs",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "init": "npm install commitizen -g && commitizen init cz-conventional-changelog --save-dev --save-exact && npm run bootstrap",
    "lib": "vue-cli-service build --target lib --name vs-ui --dest lib packages/index.js && webpack --config ./build/webpack.component.js"
  },
  "main": "lib/vs-ui.umd.js",
  "files": [
    "lib",
    "packages"
  ],
  "dependencies": {
    "core-js": "^3.7.0",
    "vant": "^2.10.13"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^4.5.6",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.1",
    "cp-cli": "^2.0.0",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.2.0",
    "element-ui": "^2.14.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.2.0",
    "highlight.js": "^9.3.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "shelljs": "^0.8.4",
    "signale": "^1.4.0",
    "sinon": "^9.2.1",
    "transliteration": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.11",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "vue",
    "vs-ui"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
